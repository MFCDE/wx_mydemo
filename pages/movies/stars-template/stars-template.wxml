<template name="starsTemplate">
    <view class="stars-container">
        <view class="stars">
            <block wx:for="{{stars}}" wx:key="item" wx:for-item="item">
                <image class="star-img" wx:if="{{item == 1}}" src="/images/icon/star.png"></image>
                <image class="star-img" wx:else src="/images/icon/none-star.png"></image>
            </block>
        </view>
        <text>{{average}}</text>
    </view>
</template>